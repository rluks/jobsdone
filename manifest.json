{
  "name": "Jobs DONE! (Red Hat Jira)",
  "version": "0.0.0.2",
  "description": "Version of 'Jobs DONE!' Chrome extension (from bugraalbayrak) for Red Hat Jira. It plays 'Jobs DONE' sound for completed Jira issues.",
  "icons": {
    "16": "assets/icon/jobs_done_16.png",
    "48": "assets/icon/jobs_done_48.png",
    "128": "assets/icon/jobs_done_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://issues.stage.redhat.com/*"
        "https://issues.redhat.com/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "*.mp3"
  ],
  "manifest_version": 2
}
